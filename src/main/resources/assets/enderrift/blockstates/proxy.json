{
  "multipart": [
    {
      "apply": { "model": "enderrift:block/proxy_main" }
    },
    {
      "when": {"up": true},
      "apply":  { "model": "enderrift:block/proxy_connection", "x": 90 }
    },
    {
      "when": {"down": true},
      "apply":  { "model": "enderrift:block/proxy_connection", "x": 270 }
    },
    {
      "when": {"east": true},
      "apply":  { "model": "enderrift:block/proxy_connection", "y": 270 }
    },
    {
      "when": {"west": true},
      "apply":  { "model": "enderrift:block/proxy_connection", "y": 90 }
    },
    {
      "when": {"north": true},
      "apply":  { "model": "enderrift:block/proxy_connection", "y": 180 }
    },
    {
      "when":  {"south": true},
      "apply":  { "model": "enderrift:block/proxy_connection", "y": 0 }
    }
  ]
}