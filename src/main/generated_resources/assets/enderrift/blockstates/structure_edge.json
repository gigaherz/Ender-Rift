{
  "multipart": [
    {
      "apply": {
        "model": "enderrift:block/straight",
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "enderrift:block/straight",
        "x": 90
      },
      "when": {
        "axis": "y"
      }
    },
    {
      "apply": {
        "model": "enderrift:block/straight"
      },
      "when": {
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "enderrift:block/base"
      },
      "when": {
        "base": "true"
      }
    }
  ]
}